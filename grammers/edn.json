{
  "name": "edn",
  "scopeName": "source.edn",
  "fileTypes": ["edn"],
  "patterns": [
    {
      "name": "comment.line.semicolon.edn",
      "match": ";.*",
      "captures": {
        "0": { "name": "comment.line.semicolon.edn" }
      }
    },
    {
      "name": "string.quoted.double.edn",
      "begin": "\"",
      "end": "\"",
      "patterns": [{ "include": "#constant.character.escape.edn" }]
    },
    {
      "name": "keyword.control.edn",
      "match": "\\b:\\w+\\b"
    },
    {
      "name": "constant.numeric.edn",
      "match": "\\b-?\\d+(\\.\\d+)?\\b"
    },
    {
      "name": "constant.language.symbol.edn",
      "match": "\\b[\\w-]+\\b"
    },
    {
      "name": "meta.bracket.square.edn",
      "begin": "\\[",
      "end": "\\]",
      "patterns": [
        { "include": "#comment" },
        { "include": "#string" },
        { "include": "#keyword" },
        { "include": "#number" },
        { "include": "#symbol" }
      ]
    },
    {
      "name": "meta.brace.edn",
      "begin": "\\{",
      "end": "\\}",
      "patterns": [
        { "include": "#comment" },
        { "include": "#string" },
        { "include": "#keyword" },
        { "include": "#number" },
        { "include": "#symbol" }
      ]
    }
  ],
  "repository": {
    "comment": {
      "patterns": [
        {
          "name": "comment.line.semicolon.edn",
          "match": ";.*$"
        }
      ]
    },
    "constant.character.escape.edn": {
      "match": "\\\\."
    },
    "keyword": {
      "patterns": [
        {
          "name": "keyword.control.edn",
          "match": "\\b:\\w+\\b"
        }
      ]
    },
    "number": {
      "patterns": [
        {
          "name": "constant.numeric.edn",
          "match": "\\b-?\\d+(\\.\\d+)?\\b"
        }
      ]
    },
    "symbol": {
      "patterns": [
        {
          "name": "constant.language.symbol.edn",
          "match": "\\b[\\w-]+\\b"
        }
      ]
    },
    "string": {
      "patterns": [
        {
          "name": "string.quoted.double.edn",
          "begin": "\"",
          "end": "\"",
          "patterns": [{ "include": "#constant.character.escape.edn" }]
        }
      ]
    }
  }
}
